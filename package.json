{
  "name": "react-template",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prod": "npm run buil && npx live-server",
    "deploy": "gh-pages -d build",
    "test": "vitest",
    "lint": "eslint .",
